<h1>Listing Users</h1>
<div class="row"> 
    <%= link "New", to: user_path(@conn, :new), class: "btn btn-success pull-right" %>
</div>
<div class="row"> 
    <table class="table">
        <%= for user <- @users do %>
            <tr>
                <td><%= render "user.html", user: user %></td>
                <td><%= link "View", to: user_path(@conn, :show, user.id) %></td>
            </tr>
        <% end %> 
    </table>
</div>
